<?xml version="1.0" encoding="UTF-8"?>
<beans>
    <!-- service bean configuration -->
    <bean title="lfMembersService" clazz="com.sparrow.app.information.service.LfMembersService">
        <property title="baseDao" ref="baseDao"/>
    </bean>
    <bean title="primarySchoolService" clazz="com.sparrow.app.information.service.PrimarySchoolService">
        <property title="baseDao" ref="baseDao"/>
    </bean>

    <!-- command bean configuration -->
    <bean title="lfMembersCmd" parameter="lf/lf_members" clazz="com.sparrow.app.information.command.LfMembersCommand" init="initialize">
        <property title="lfMembersService" ref="lfMembersService"/>
        <property title="operateTemplate" ref="operateTemplate" />
        <property title="dataToolCommand" ref="dataToolCommand" />
    </bean>
    <bean title="primarySchoolCmd" parameter="primary/primary_school"
          clazz="com.sparrow.app.information.command.PrimarySchoolCommand">
        <property title="primarySchoolService" ref="primarySchoolService"/>
    </bean>
</beans>
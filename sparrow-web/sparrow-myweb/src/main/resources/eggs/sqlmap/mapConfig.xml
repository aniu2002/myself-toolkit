<?xml version="1.0" encoding="UTF-8"?>
<maps>
	<map table="lf_members" clazz="com.sparrow.app.information.domain.LfMembers" desc="">
		<key>id</key>
		<select>
			SELECT * FROM lf_members WHERE id=?
		</select>
		<insert>
			INSERT INTO lf_members(title,qq,sex,age,bra,phone,province,city,district,referee_name,referee_qq,price_p,price_pp,price_desc,simple_desc,special,checked,leval,comment,images,create_date,mark) VALUES(:title,:qq,:sex,:age,:bra,:phone,:province,:city,:district,:refereeName,:refereeQq,:priceP,:pricePp,:priceDesc,:simpleDesc,:special,:checked,:leval,:comment,:images,:createDate,:mark)
		</insert>
		<update>
			UPDATE lf_members SET title=:title,qq=:qq,sex=:sex,age=:age,bra=:bra,phone=:phone,province=:province,city=:city,district=:district,referee_name=:refereeName,referee_qq=:refereeQq,price_p=:priceP,price_pp=:pricePp,price_desc=:priceDesc,simple_desc=:simpleDesc,special=:special,checked=:checked,leval=:leval,comment=:comment,update_date=:updateDate,mark=:mark WHERE id=:id
		</update>
		<delete>
			DELETE FROM lf_members WHERE id=?
		</delete>
		<query>
			SELECT * FROM lf_members
		</query>
	</map>
    <map table="primary_school" clazz="com.sparrow.app.information.domain.PrimarySchool" desc="">
        <key>id</key>
        <select>
            SELECT * FROM primary_school WHERE id=?
        </select>
        <insert>
            INSERT INTO primary_school(open_id,title,phone,nick_name,sex,language,city,province,country,head_image) VALUES(:openId,:title,:phone,:nickName,:sex,:language,:city,:province,:country,:headImage)
        </insert>
        <update>
            UPDATE primary_school SET open_id=:openId,title=:title,phone=:phone,nick_name=:nickName,sex=:sex,language=:language,city=:city,province=:province,country=:country,head_image=:headImage WHERE id=:id
        </update>
        <delete>
            DELETE FROM primary_school WHERE id=?
        </delete>
        <query>
            SELECT * FROM primary_school
        </query>
    </map>
</maps>